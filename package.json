{
  "name": "angular-seed-ultimate",
  "version": "0.1.0",
  "description": "Webpack2 driven Angular2 ngrx Multi-Platform Seed",
  "repository": {
    "url": "git@github.com:DeviantJS/angular-seed-ultimate.git",
    "type": "git"
  },
  "contributors": [
    {
      "name": "Joshua Wiens <@d3viant0ne>"
    },
    {
      "name": "Nathan Walkker <@NathanWalker>"
    }
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 0",
    "e2e": "echo \"Error: no test specified\" && exit 0",
    "start": "yarn run server:dev",
    "start:hmr": "yarn run server:dev -- --env.HMR",
    "server": "yarn run server:dev",
    "server:dev": "webpack-dev-server --env.ENV development --inline --progress --profile --watch",
    "server:prod": "http-server dist --cors",
    "debug:start": "node-nightly --inspect --debug-brk node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.ENV development",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js --env.ENV development",
    "rimraf": "rimraf",
    "lint": "tslint \"src/**/*.ts\"",
    "tslint": "tslint",
    "prebuild": "rimraf dist/*.*",
    "build": "yarn run build:dev",
    "build:dev": "webpack --env.ENV development  --progress --profile",
    "build:prod": "webpack --env.ENV development   --progress --profile --bail",
    "build:hmr": "yarn run build -- --env.HMR",
    "predll": "rimraf dist/dll/*.*",
    "dll": "webpack --config webpack.dll.ts --env.ENV development",
    "watch:dev:hmr": "yarn run watch:dev -- --hot",
    "watch:dev": "yarn run build:dev -- --watch",
    "watch:prod": "yarn run build:prod -- --watch",
    "watch:test": "yarn run test -- --auto-watch --no-single-run",
    "watch": "yarn run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "yarn run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "scan:security": "nsp check",
    "report-codeclimate": "codeclimate-test-reporter < coverage/lcov.info",
    "ci": "yarn run lint && yarn test && yarn run e2e",
    "release": "standard-version"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.46",
    "assets-webpack-plugin": "^3.4.0",
    "awesome-typescript-loader": "^2.2.1",
    "codelyzer": "^1.0.0-beta.3",
    "commitizen": "^2.8.6",
    "copy-webpack-plugin": "^4.0.0",
    "cross-spawn": "^4.0.0",
    "cz-conventional-changelog": "^1.2.0",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ie-shim": "^0.1.0",
    "ignore-loader": "^0.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.11.2",
    "nsp": "^2.6.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "standard-changelog": "^0.0.1",
    "standard-version": "^3.0.0",
    "string-replace-loader": "github:gdi2290/string-replace-loader",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "github:gdi2290/ts-helpers",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.2.2",
    "tslint": "^3.15.1",
    "typescript": "2.0.2",
    "url-loader": "^0.5.7",
    "webpack": "~2.1.0-beta.25",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^2.1.0-beta.8"
  },
  "dependencies": {
    "@angular/common": "^2.1.2",
    "@angular/compiler": "^2.1.2",
    "@angular/compiler-cli": "^2.1.2",
    "@angular/core": "^2.1.2",
    "@angular/forms": "^2.1.2",
    "@angular/http": "^2.1.2",
    "@angular/platform-browser": "^2.1.2",
    "@angular/platform-browser-dynamic": "^2.1.2",
    "@angular/platform-server": "^2.1.2",
    "@angular/router": "^3.1.2",
    "@angularclass/conventions-loader": "^1.0.12",
    "@angularclass/form-validators": "^1.0.11",
    "@angularclass/hmr": "~1.2.0",
    "@angularclass/hmr-loader": "~3.0.1",
    "@angularclass/resolve-angular-routes": "^1.0.8",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.1",
    "@ngrx/store-log-monitor": "^3.0.2",
    "core-js": "^2.4.1",
    "ngrx-store-freeze": "^0.1.4",
    "ngrx-store-logger": "^0.1.7",
    "rxjs": "^5.0.0-rc.1",
    "zone.js": "^0.6.26"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "dist",
    "src"
  ]
}
